{
  "name": "call-duration-measurer",
  "version": "1.5.0",
  "description": "call-duration-measurer is a utility for measuring the time it takes for function calls to complete.",
  "main": "index.js",
  "engines": {
    "node": ">=12.x"
  },
  "author": "Raymond Tang",
  "license": "MIT",
  "repository": "https://github.com/raymondtang310/call-duration-measurer.git",
  "private": true,
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn clean && webpack && node scripts/addFilesToDistribution.js",
    "test": "yarn jest",
    "lint": "eslint --ignore-path .gitignore --ignore-pattern node_modules . --ext .ts",
    "lint:fix": "yarn lint --fix",
    "version": "auto-changelog -p && git add CHANGELOG.md",
    "create-tar": "yarn build && cd dist && yarn pack --filename call-duration-measurer.tgz && move-file call-duration-measurer.tgz ../call-duration-measurer.tgz",
    "create-link": "yarn build && cd dist && yarn link",
    "prepush": "yarn lint && yarn test",
    "prepublishOnly": "yarn prepush && yarn build",
    "publish-package": "yarn publish dist"
  },
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/preset-env": "7.9.0",
    "@babel/register": "7.9.0",
    "@commitlint/cli": "9.1.2",
    "@commitlint/config-conventional": "9.1.2",
    "@typescript-eslint/eslint-plugin": "4.14.2",
    "@typescript-eslint/parser": "4.14.2",
    "auto-changelog": "2.2.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.2.4",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-import-resolver-webpack": "0.12.1",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jest": "24.1.3",
    "eslint-plugin-no-only-tests": "2.4.0",
    "eslint-plugin-prettier": "3.1.2",
    "husky": "4.2.5",
    "jest": "26.6.3",
    "jest-junit": "10.0.0",
    "lint-staged": "10.1.1",
    "move-file-cli": "2.0.0",
    "prettier": "2.0.2",
    "rimraf": "3.0.2",
    "ts-jest": "26.5.0",
    "ts-loader": "8.0.14",
    "ts-node": "9.1.1",
    "tsconfig-paths-webpack-plugin": "3.3.0",
    "ttypescript": "1.5.12",
    "typescript": "4.1.3",
    "typescript-transform-paths": "2.2.3",
    "webpack": "4.42.1",
    "webpack-cli": "4.2.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-push": "yarn prepush",
      "post-merge": "yarn"
    }
  },
  "lint-staged": {
    "*.js": [
      "yarn lint:fix"
    ]
  },
  "sideEffects": false,
  "types": "index.d.ts"
}
